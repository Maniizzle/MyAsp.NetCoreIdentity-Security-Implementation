@model Dictionary<string, object>




@{
    ViewData["Title"] = "Index";
}

<div class="bg-primary m-1 p-1 text-white"><h4>User Detail</h4></div>


<table class="table table-sm table-bordered m-1 p-1">
    @foreach (var kvp in Model)
    {
        <tr>
            <th>@kvp.Key</th>
            <td> @kvp.Value</td>

        </tr>
    }
    @if (User?.Identity?.IsAuthenticated ?? false)
    {
        <a asp-controller="MyAccount" asp-action="LogOut" class="btn btn-danger">Logout</a>
    }
    else {
    <a asp-controller="MyAccount" asp-action="Login" class="btn btn-danger">LogIn</a>
    }
</table>


